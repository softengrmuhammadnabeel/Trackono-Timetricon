{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "TrackOno",
  "version": "0.1.0",
  "identifier": "com.trackono.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "plugins": {
    "screenshots": {},
    "fs": {
      "requireLiteralLeadingDot": false
    }

  },
  
  "app": {
    "windows": [
      {
        "title": "TrackOno",
        "width": 800,
        "height": 600,
        "minHeight": 600,
        "minWidth": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/256x256.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}